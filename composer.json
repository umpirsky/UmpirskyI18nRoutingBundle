{
    "name": "umpirsky/i18n-routing-bundle",
    "type": "symfony-bundle",
    "description": "Internationalized routing with minimal performance cost. ",
    "keywords": ["routing", "i18n", "internationalisation", "translation", "Symfony"],
    "license": "MIT",
    "authors": [
        {
            "name": "Saša Stamenković",
            "email": "umpirsky@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "symfony/framework-bundle": "^5.4|^6.0|^7.0"
    },
    "require-dev": {
        "symfony/var-dumper": "^5.4|^6.0|^7.0",
        "symfony/phpunit-bridge": "^6.0|^7.0",
        "symfony/yaml": "^5.0|^6.0|^7.0",
        "phpunit/phpunit": "^9.6.13"
    },
    "autoload": {
        "psr-4": { "Umpirsky\\I18nRoutingBundle\\": "src/" }
    },
    "autoload-dev": {
        "files": ["tests/fixtures/app/AppKernel.php"]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.0.1-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
