{
    "name": "umpirsky/i18n-routing-bundle",
    "type": "symfony-bundle",
    "description": "Internationalized routing with minimal performance cost. ",
    "keywords": ["routing", "i18n", "internationalisation", "translation", "Symfony"],
    "license": "MIT",
    "authors": [
        {
            "name": "Saša Stamenković",
            "email": "umpirsky@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2|^8.0",
        "symfony/framework-bundle": "^4.4|^5.0|^6.0"
    },
    "require-dev": {
        "symfony/var-dumper": "^4.4|^5.0|^6.0",
        "symfony/phpunit-bridge": "^5.3",
        "bamarni/composer-bin-plugin": "^1.4",
        "symfony/yaml": "^4.4|^5.0|^6.0"
    },
    "autoload": {
        "psr-4": { "Umpirsky\\I18nRoutingBundle\\": "src/" }
    },
    "autoload-dev": {
        "files": ["tests/fixtures/app/AppKernel.php"]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.0.1-dev"
        },
        "bamarni-bin": {
            "target-directory": "bin/tools"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
